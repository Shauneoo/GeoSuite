<div class="row">
  <div class="col-sm-8">
    <table class="table table-bordered table-hover table-striped">
      <thead>
        <tr>
          <th>Event Type</th>
          <th>Value</th>
          <th>Lat</th>
          <th>Lng</th>
          <th>Radius</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <% for (var event of events) { %>
          <tr>
            <td><%= event.type %></td>
            <td><%= event.value %></td>
            <td><%= event.lat %></td>
            <td><%= event.lng %></td>
            <td><%= event.radius %></td>
            <form action="/event/delete" method="POST">
              <td><button type="submit" name="deleteEvent" value="<%= event.id %>">Delete</button></td>
            </form>
          </tr>
        <% } %>
        <tr>
          <form action="/event/create" method="POST">
            <div class="form-group">
              <td>
                <select class="form-control" type="text" name="type">
                  <option>coinCache</option>
                  <option>toll</option>
                  <option>passiveEarn</option>
                  <option>passiveTax</option>
                </select>
              </td>
              <td><input type="number" name="value"></td>
              <td><input name="lat"></td>
              <td><input name="lng"></td>
              <td><input type="number" name="radius"></td>
            </div>
            <div class="form-group">
              <td><button type="submit" name="newEventButton">Create</button></td>
            </div>
          </form>
        </tr>
      </tbody>
    </table>
  </div>
  <div class="col-sm-4">
    <div id="previewCoords"></div>
    <div id="map"></div>
</div>
<script>window.onload = function(){ adminPos(); }</script>
